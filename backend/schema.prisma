datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @id @default(cuid())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
}

model HomePage {
  id                    String    @id @default(cuid())
  slug                  String    @default("home")
  heroTitle_en          String    @default("")
  heroTitle_ar          String    @default("")
  heroDescription_en    String    @default("")
  heroDescription_ar    String    @default("")
  missionTitle_en       String    @default("")
  missionTitle_ar       String    @default("")
  missionDescription_en String    @default("")
  missionDescription_ar String    @default("")
  aboutUsTitle_en       String    @default("")
  aboutUsTitle_ar       String    @default("")
  aboutUsDescription_en String    @default("")
  aboutUsDescription_ar String    @default("")
  contactInfo_en        String    @default("")
  contactInfo_ar        String    @default("")
  createdAt             DateTime?
  updatedAt             DateTime?
}
